<!DOCTYPE html>
<html lang="en">
<head>
    <title>Trippy Land</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link href="https://fonts.googleapis.com/css?family=Poppins:300,400,500,600,700,800,900" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css2?family=Arizonia&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">
    <link rel="stylesheet" href="/css/animate.css">
    <link rel="stylesheet" href="~/Trippy_Land_Theme/css/animate.css">
    <link rel="stylesheet" href="~/Trippy_Land_Theme/css/owl.carousel.min.css">
    <link rel="stylesheet" href="~/Trippy_Land_Theme/css/owl.theme.default.min.css">
    <link rel="stylesheet" href="~/Trippy_Land_Theme/css/magnific-popup.css">
    <link rel="stylesheet" href="~/Trippy_Land_Theme/css/bootstrap-datepicker.css">
    <link rel="stylesheet" href="~/Trippy_Land_Theme/css/jquery.timepicker.css">
    <link rel="stylesheet" href="~/Trippy_Land_Theme/css/flaticon.css">
    <link rel="stylesheet" href="~/Trippy_Land_Theme/css/style.css">
</head>
<body>
    @Html.Partial("_Navigation")

    <!-- END nav -->

    <section class="hero-wrap hero-wrap-2 js-fullheight" style="background-image: url('@Url.Content("~/Trippy_Land_Theme/images/bg_1.jpg")');">
        <div class="overlay"></div>
        <div class="container">
            <div class="row no-gutters slider-text js-fullheight align-items-end justify-content-center">
                <div class="col-md-9 ftco-animate pb-5 text-center">
                    <p class="breadcrumbs"><span class="mr-2"><a href="~/home/index">Home <i class="fa fa-chevron-right"></i></a></span> <span>About us <i class="fa fa-chevron-right"></i></span></p>
                    <h1 class="mb-0 bread">About Us</h1>
                </div>
            </div>
        </div>
    </section>

    @Html.Partial("_IntroQuotes")
    <section class="ftco-section ftco-about img" style="background-image: url('@Url.Content("~/Trippy_Land_Theme/images/bg_4.jpg")');">
        <div class="overlay"></div>
        <div class="container py-md-5">
            <div class="row py-md-5">
                <div class="col-md d-flex align-items-center justify-content-center">
                    <a href="https://vimeo.com/45830194" class="icon-video popup-vimeo d-flex align-items-center justify-content-center mb-4">
                        <span class="fa fa-play"></span>
                    </a>
                </div>
            </div>
        </div>
    </section>

    @Html.Partial("_About")


    <section class="ftco-section ftco-no-pb contact-section mb-4">
        <div class="container">
            <div class="row d-flex contact-info">
                <div class="col-md-3 d-flex">
                    <div class="align-self-stretch box p-4 text-center">
                        <div class="icon d-flex align-items-center justify-content-center">
                            <span class="fa fa-map-marker"></span>
                        </div>
                        <h3 class="mb-2">Address</h3>
                        <p>Thanh Xuan District, Ha Noi City, Viet Nam</p>
                    </div>
                </div>
                <div class="col-md-3 d-flex">
                    <div class="align-self-stretch box p-4 text-center">
                        <div class="icon d-flex align-items-center justify-content-center">
                            <span class="fa fa-phone"></span>
                        </div>
                        <h3 class="mb-2">Contact Number</h3>
                        <p><div>+849886880704</div></p>
                    </div>
                </div>
                <div class="col-md-3 d-flex">
                    <div class="align-self-stretch box p-4 text-center">
                        <div class="icon d-flex align-items-center justify-content-center">
                            <span class="fa fa-paper-plane"></span>
                        </div>
                        <h3 class="mb-2">Email Address</h3>
                        <p><div>vuphamhocvien<br />matma@gmail.com</div></p>
                    </div>
                </div>
                <div class="col-md-3 d-flex">
                    <div class="align-self-stretch box p-4 text-center">
                        <div class="icon d-flex align-items-center justify-content-center">
                            <span class="fa fa-globe"></span>
                        </div>
                        <h3 class="mb-2">Website</h3>
                        <p><div href="#">trippyland.com</div></p>
                    </div>
                </div>
            </div>
        </div>
    </section>
    @Html.Partial("_OurBlog")
    @Html.Partial("_AskForQuote")
    @Html.Partial("_Footer")

    <!-- loader -->
    <!-- loader -->
    <div id="ftco-loader" class="show fullscreen"><svg class="circular" width="48px" height="48px"><circle class="path-bg" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke="#eeeeee" /><circle class="path" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke-miterlimit="10" stroke="#F96D00" /></svg></div>
    <script src="~/Trippy_Land_Theme/js/jquery.min.js"></script>
    <script src="~/Trippy_Land_Theme/js/jquery-migrate-3.0.1.min.js"></script>
    <script src="~/Trippy_Land_Theme/js/popper.min.js"></script>
    <script src="~/Trippy_Land_Theme/js/bootstrap.min.js"></script>
    <script src="~/Trippy_Land_Theme/js/jquery.easing.1.3.js"></script>
    <script src="~/Trippy_Land_Theme/js/jquery.waypoints.min.js"></script>
    <script src="~/Trippy_Land_Theme/js/jquery.stellar.min.js"></script>
    <script src="~/Trippy_Land_Theme/js/owl.carousel.min.js"></script>
    <script src="~/Trippy_Land_Theme/js/jquery.magnific-popup.min.js"></script>
    <script src="~/Trippy_Land_Theme/js/jquery.animateNumber.min.js"></script>
    <script src="~/Trippy_Land_Theme/js/bootstrap-datepicker.js"></script>
    <script src="~/Trippy_Land_Theme/js/scrollax.min.js"></script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBVWaKrjvy3MaE7SQ74_uJiULgl1JY0H2s&sensor=false"></script>
    <script src="~/Trippy_Land_Theme/js/google-map.js"></script>
    <script src="~/Trippy_Land_Theme/js/main.js"></script>
</body>
</html>